.navRow {
    height: 47px;
    width: 100%;
    display: flex;
    background-color: #FFFFFF;
    
}
.yellowRetangle {
    width: 9px;
    height: 47px;
    background-color: rgba(252, 236, 83, 1);
    border-radius: 0px 0px 2px 0px;
}

.pinkRetangle {

    width: 9px;
    height: 17px;
    opacity: 1;
    background: rgba(252, 228, 233, 1);
    margin-left: 2px;
    border-radius: 0px 0px 2px 2px;
}

.photo {
    border-radius: 0px 2px 2px 2px;
    margin-left: 2px;
    width: 47px;
    height: 47px;
    opacity: 1;
    background: rgba(229, 229, 229, 1);
}

.imageRadius {
    border-radius: 0px 2px 2px 2px;
}
.whiteRectangle{
    height: 17px;
    
}
.right{
    width: 100%;
}
.navTab {
   
    height:30px;
    opacity: 0.5;
    border-radius: 2px 0px 0px 2px;
    background: rgba(250, 202, 213, 1);
    margin-left: 4px;
    z-index: 1;
}

.context{
    margin-top: 17px;
    width: 100%;
}
.cell{
    position: flex;
    height: 35px;
}
.myicon{
    margin-top: 3px;
